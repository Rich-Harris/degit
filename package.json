    "tar": "4.4.10"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.18.2",
    "glob": "7.1.4",
    "mocha": "6.2.0",
    "rimraf": "3.0.0",
    "rollup": "1.19.4",
    "rollup-plugin-commonjs": "10.0.0",
